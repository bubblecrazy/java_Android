<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">
<servlet>
    <servlet-name>Database</servlet-name>
    <servlet-class>servlet.Database</servlet-class>
</servlet>
    <servlet-mapping>
        <servlet-name>Database</servlet-name>
        <url-pattern>/servlet/Database</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FileDB</servlet-name>
        <servlet-class>servlet.FileDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileDB</servlet-name>
        <url-pattern>/servlet/FileDB</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ShowManager</servlet-name>
        <servlet-class>servlet.ShowManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowManager</servlet-name>
        <url-pattern>/servlet/ShowManager</url-pattern>
    </servlet-mapping>
</web-app>
